{
  "name": "monday-forums",
  "version": "1.0.0",
  "transform": {
    "^.+\\.tsx?$": "ts-jest",
    ".+\\.(css|styl|less|sass|scss|png|jpg|svg|ttf|woff|woff2)$": "jest-transform-stub"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "scripts": {
    "start": "npm run dev && npm run expose",
    "dev": "cross-env MONDAY_ENVIRONMENT=dev parcel ./index.html --open --no-cache --port 6012",
    "staging": "cross-env MONDAY_ENVIRONMENT=staging parcel ./index.html --open --no-cache --port 6012",
    "production": "cross-env MONDAY_ENVIRONMENT=production parcel ./index.html --open --no-cache --port 6012",
    "build": "cross-env MONDAY_ENVIRONMENT=production parcel build ./index.html --no-cache",
    "builddev": "cross-env MONDAY_ENVIRONMENT=staging parcel build ./index.html --no-cache",
    "expose": "ngrok http 6012",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.158",
    "@types/moment": "^2.13.0",
    "@types/monday-sdk-js": "^0.1.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.44",
    "@types/react-beautiful-dnd": "^12.1.3",
    "@types/react-custom-scrollbars": "^4.0.6",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-select": "^1.3.4",
    "@types/react-textarea-autosize": "^4.3.5",
    "@types/react-transition-group": "^1.1.6",
    "axios": "^0.18.1",
    "babel-plugin-emotion": "^10.0.33",
    "cross-env": "^5.2.1",
    "element-resize-detector": "^1.2.1",
    "emotion": "^9.2.12",
    "formik": "^1.5.8",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "monday-sdk-js": "^0.1.1",
    "npm": "^6.14.7",
    "parcel-bundler": "^1.12.4",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^12.2.0",
    "react-container-dimensions": "^1.4.1",
    "react-contenteditable": "^3.3.5",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-markdown": "^5.0.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.3.0",
    "react-syntax-highlighter": "^15.3.1",
    "react-textarea-autosize": "^7.1.2",
    "react-transition-group": "^1.2.1",
    "unstated": "^2.1.1",
    "yup": "^0.25.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "@types/react-test-renderer": "16.0.0",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^24.9.0",
    "jest-junit": "^7.0.0",
    "jest-transform-stub": "^2.0.0",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.9.7"
  }
}
